<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE checkstyle-metadata PUBLIC
"-//eclipse-cs//DTD Check Metadata 1.1//EN"
"http://eclipse-cs.sourceforge.net/dtds/checkstyle-metadata_1_1.dtd">
<checkstyle-metadata>
    <rule-group-metadata name="My custom checks"  priority="1600">
        <description>%MyCustomChecks.desc</description>

        <rule-metadata name="%MethodLimit.name" internal-name="MethodLimit" parent="TreeWalker">
            <alternative-name internal-name="net.sf.eclipsecs.sample.checks.MethodLimitCheck" />
            <description>%MethodLimit.desc</description>
            <property-metadata name="max" datatype="Integer" default-value="30">
                <description>The maximum number of allowed methods.</description>
            </property-metadata>
            <quickfix classname="net.sf.eclipsecs.sample.checks.MethodLimitQuickfix" />
        </rule-metadata>
        <rule-metadata name="%UselessType.name" internal-name="UselessType" parent="TreeWalker">
        	<alternative-name internal-name="net.sf.eclipsecs.sample.checks.UselessTypeCheck"></alternative-name>
        	<description>%UselessType.desc</description></rule-metadata>
        <rule-metadata name="%WholePart.name" internal-name="WholePart" parent="TreeWalker">
        	<alternative-name internal-name="net.sf.eclipsecs.sample.checks.WholePartCheck"></alternative-name>
        	<description>%WholePart.desc</description></rule-metadata>
        <rule-metadata name="%MeaninglessTerm.name" internal-name="MeaninglessTerm" parent="TreeWalker">
        	<alternative-name internal-name="net.sf.eclipsecs.sample.checks.MeaninglessTermCheck"></alternative-name>
        	<description>%MeaninglessTerm.desc</description></rule-metadata>
        <rule-metadata name="%Spelling.name" internal-name="Spelling" parent="TreeWalker">
        	<alternative-name internal-name="net.sf.eclipsecs.sample.checks.SpellingCheck"></alternative-name>
        	<description>%Spelling.desc</description></rule-metadata>
   	</rule-group-metadata>
</checkstyle-metadata>
